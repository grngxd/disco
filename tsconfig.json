{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "Preserve",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "jsxImportSource": "./core/jsx-runtime/jsx-runtime",
    "allowJs": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false
  },
  "include": [
    "core/global.d.ts",
    "core/jsx-runtime/**/*.ts",
    "core/types.ts",
    "core/elements.ts",
    "core/renderer.ts",
    "core/disco.ts",
    "hooks/useSignal.ts",
    "hooks/useEffect.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}